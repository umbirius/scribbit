<%= form_for @sett, url: project_sett_path(params[:project_id], @sett), html: { method: :patch } do |f| %> 
    <%= f.label :name %> 
    <%= f.text_field :name %><br>
    <%= f.label :location %> 
    <%= f.text_field :location%><br>
    <%= f.label :description %> 
    <%= f.text_field :description %><br>
    <%= f.submit %>
<% end %>
<%= form_for @character, url: @url, html: { method: :patch } do |f| %> 
    <% if !params[:project_id] %>
        <%= f.label :project %> 
        <%= f.collection_select :project, current_user.projects.order(:title), :title, :title, :selected => @character.project.title %><br>
    <% end %> 
    <%= f.label :name %> 
    <%= f.text_field :name %><br>
    <%= f.label :age %> 
    <%= f.text_field :age%><br>
    <%= f.label :bio %> 
    <%= f.text_field :bio %><br>
    <%= f.submit %>
<% end %>
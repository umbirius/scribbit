<%= form_for @character, url: project_character_path(@character), html: { method: :patch } do |f| %> 
    <%= f.label :name %> 
    <%= f.text_field :name %><br>
    <%= f.label :age %> 
    <%= f.text_field :age%><br>
    <%= f.label :bio %> 
    <%= f.text_field :bio %><br>
    <%= f.submit %>
<% end %>
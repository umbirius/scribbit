<%= form_for @project do |f| %> 
    <%= f.label :title %> 
    <%= f.text_field :title %><br>
    <%= f.label :form %> 
    <%= f.text_field :form%><br>
    <%= f.label :genre %> 
    <%= f.text_field :genre %><br>
    <%= f.label :description %> 
    <%= f.text_field :description %><br>
    <%= f.submit %>
<% end %>

<%= button_to "Delete Project", project_path(params[:id]), method: :delete %>
<%= link_to 'Back to Projects',  projects_path %>

<%= form_for @project do |f| %> 
    <%= f.label :title %><br> 
    <%= f.text_field :title %><br>
    <%= f.label :form %> 
    <%= f.text_field :form%><br>
    <%= f.label :genre %> 
    <%= f.text_field :genre %><br>
